{	
  "presets": [	
    [	
      "env",	
      {	
        "loose": true,	
        "exclude": ["transform-es2015-typeof-symbol"],	
        "targets": {	
          "browsers": ["last 2 versions", "IE >= 9"]	
        }	
      }	
    ]	
  ],	
  "plugins": [	
    "transform-class-properties",	
    ["transform-react-jsx", { "pragma": "Omi.h" }],	
    "transform-object-rest-spread",	
    "syntax-dynamic-import"	
  ]	
}